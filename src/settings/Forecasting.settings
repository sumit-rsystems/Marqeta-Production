    <adjustmentsSettings>
        <enableAdjustments>true</enableAdjustments>
    </adjustmentsSettings>
    <dataSourceSettings>
        <enableQuantity>false</enableQuantity>
        <enableRevenue>true</enableRevenue>
    </dataSourceSettings>
    <enableForecasts>true</enableForecasts>
