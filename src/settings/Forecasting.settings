        <field>OPPORTUNITY.CLOSE_DATE</field>
        <field>OPPORTUNITY.STAGE_NAME</field>
        <field>OPPORTUNITY.PROBABILITY</field>
        <field>FORECAST_CATEGORY</field>
        <field>FULL_NAME</field>
    </opportunityListFieldsSelectedSettings>
